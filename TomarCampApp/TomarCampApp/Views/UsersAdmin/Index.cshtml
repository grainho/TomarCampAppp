@model IEnumerable<TomarCampApp.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>

                @Html.ActionLink(item.UserName, "Details", new { id = item.Id }) |@Html.ActionLink("✏️", "Edit", new { id = item.Id }) |
                @Html.ActionLink("🗑️", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
